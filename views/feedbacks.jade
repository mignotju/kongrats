extends layout

block stylesheets
  link(rel='stylesheet', href='/stylesheets/style_feedbacks.css')

block content
  button.btn.btn-secondary(type='button', onclick=onclick="javascript:window.location.href  = '/matieres/'") Retour sur la page mati√®res
  h2 #{annee} - #{matiere} :
  table.table.table-bordered
    thead
      tr
        each question in feedbacks[0]
          th #{question}
    tbody
        each row, k in feedbacks
          if (k != 0)
            tr
              - for (var i = 0; i < row.length; i++)
                td #{row[i]}
